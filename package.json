{
    "name": "horizon-extended",
    "displayName": "Horizon Extended Theme",
    "description": "A dark theme based on the Horizon theme. This provides some much need additions included semantic highlighting.",
    "version": "1.1.0",
    "publisher": "lancewilhelm",
    "engines": {
        "vscode": "^1.55.0"
    },
    "icon": "logo.png",
    "author": {
        "name": "Lance Wilhelm",
        "email": "lance.wilhelm@gmail.com",
        "url": "https://lancewilhelm.com"
    },
    "repository": {
        "url": "https://github.com/lancewilhelm/horizon-extended",
        "type": "git"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "theme",
        "dark",
        "horizon",
        "extended",
        "warm",
        "pink",
        "red"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Horizon Extended - Neo",
                "uiTheme": "vs-dark",
                "path": "./themes/neo.json"
            },
            {
                "label": "Horizon Extended - Light Neo",
                "uiTheme": "vs",
                "path": "./themes/light_neo.json"
            },
            {
                "label": "Horizon Extended - Original",
                "uiTheme": "vs-dark",
                "path": "./themes/original.json"
            },
            {
                "label": "Horizon Extended - Original No Italics",
                "uiTheme": "vs-dark",
                "path": "./themes/original_no_italics.json"
            },
            {
                "label": "Horizon Extended - Light Original",
                "uiTheme": "vs",
                "path": "./themes/light_original.json"
            }
        ]
    }
}
